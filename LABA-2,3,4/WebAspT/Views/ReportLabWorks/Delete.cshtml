        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.id_Project)
        </dd>
        <dt class = "col-sm-2">
            @*@Html.DisplayNameFor(model => model.id_Student)*@
            номер студента
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.id_Student)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="id_ReportLW" />
        <input type="submit" value="Удалить" class="btn btn-danger" /> |
        <a asp-action="Index">К списку</a>
    </form>
</div>
